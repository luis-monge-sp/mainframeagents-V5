//UNTRS    EXEC PGM=TRSMAIN,PARM=UNPACK
//SYSPRINT DD SYSOUT=*
//INFILE   DD DISP=SHR,DSN=CTS.BKP.PRECLS.D240305.BS10132.TRS
//OUTFILE  DD DSN=CTS.BKP.PRECLS.D240305.BS10132,
//         DISP=(NEW,CATLG),SPACE=(CYL,(900,250),RLSE)
//*
//RESTDEV EXEC PGM=ADRDSSU,REGION=0M,COND=(0,NE)   ,PARM='TYPRUN=NORUN'
//RESTORE DD DISP=SHR,DSN=CTS.BKP.PRECLS.D240305.BS10132
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 RESTORE INDD(RESTORE) -
         DS(INCL(**)  -
            EXCL(SCM.PKG.**)) -
         REPLACE       -
         CANCELERROR
