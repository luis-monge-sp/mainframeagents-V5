                ONE_FIELD_rec_typ         * pair,
                ESAADI_LIST_OPEN_MODE_typ   mode,
                short                       max_list_length,
                ESAADIL_LIST_HNDL_typ     * handle);

@**************************************************************
*                                                             *
* PROCEDURE NAME : ESAADIL_list_go_next_entry                 *
*                                                             *
