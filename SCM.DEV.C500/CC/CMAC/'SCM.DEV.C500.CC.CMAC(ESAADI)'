 /* WS10082K - start                        */
 /*  sizeof(ADDINFO_rec_typ)   addinfo header including 1 pair
  *                +
  * (num-1)*sizeof(ONE_FIELD_rec_typ))   pairs               */
#define ADDINFO_SIZE(num)                                   \
              ( sizeof(ADDINFO_rec_typ)+                    \
              ((num-1)*sizeof(ONE_FIELD_rec_typ)))

/****************************************************/
/*                                                  */
/*                ADDINFO_INIT                      */
/*                                                  */
/*  Initialize a newly allocated addinfo, or the    */
/*  unused entries when realloc is done.            */
/*                                                  */
/*  parameters:                                     */
/*  -  ai - addinfo.                                */
/*  -  max_pairs - # of addinfo entries.            */
/*  -  num_pairs - # of occupied entries.           */
/*                                                  */
/*  REALLOC callers:                                */
/*  The caller is responsible to take care of the   */
/*  twin-pair pointers.                             */
/*                                                  */
/****************************************************/
#define ADDINFO_INIT(ai, _max_pairs, _num_pairs, _hi, _comp)     \
{                                                                \
  if (_num_pairs EQ 0)        /* only if initial alloc  */       \
  {                                                              \
    ai->num_pairs = _num_pairs;                                  \
    ai->heap_id = _hi;                                           \
  }                                                              \
                                                                 \
  /* always - set max_pairs                     */               \
  ai->max_pairs = _max_pairs;                                    \
                                                                 \
  /* Initalize all new entries   */                              \
  ESA_DIAG_printf(_comp, 9,                                      \
                   "Going to init pairs %d - %d",                \
                      _num_pairs, _max_pairs-1);                 \
  for (int _ii=_num_pairs; _ii LT _max_pairs; _ii++) {           \
      ADDINFO_INIT_PAIR(&ai->pair[_ii], _ii, 1)                  \
  /*  WS10082N                                                   \
      ai->pair[_ii].twin = &(ai->pair[_ii]);                     \
      ai->pair[_ii].index = _ii;                                 \
      ai->pair[_ii].secret = 'N';                                \
      ai->pair[_ii].addval_msg = 'N';                            \
      ai->pair[_ii].SLType = 'S';                                \
      ai->pair[_ii].keylen = 0;                                  \
      ai->pair[_ii].keyword[0] = NULL_CHAR;                      \
      ai->pair[_ii].vallen = 0;                                  \
      ai->pair[_ii].value[0] = NULL_CHAR;                 */     \
   }                                                             \
 }                                                               \
/* WS10082K - End*/

/*  WS10082N- start */
/****************************************************/
/*                                                  */
/*                ADDINFO_INIT_PAIR                 */
/*                                                  */
/*  Initialize a pair.                              */
/*                                                  */
/*  parameters:                                     */
/*  -  pair - ->pair.                               */
/*  -  pi - pair index.                             */
/*  -  all - 1 - all fields; 0 - value related only */
/*                                                  */
/*                                                  */
/****************************************************/
#define ADDINFO_INIT_PAIR(_pair, _pi, _all)                      \
{                                                                \
  if (_all)  {                                                   \
    (_pair)->index = _pi;                                        \
    (_pair)->secret = 'N';                                       \
    (_pair)->keylen = 0;                                         \
    (_pair)->keyword[0] = NULL_CHAR;                             \
  }                                                              \
  (_pair)->SLType = 'S';                                         \
  (_pair)->addval_msg = 'N';                                     \
  (_pair)->twin = (_pair);                                       \
  (_pair)->vallen = 0;                                           \
  (_pair)->value[0] = NULL_CHAR;                                 \
}                                                                \
  /*  WS10082N- end   */


/* WS10082 removed now redundant
#define ADDINFO_value_addval           adies67
#define ERROR_dump                     adies38
#define ERROR_add                      adies39
#define ERROR_print                    adies40
   ws10082 end */

