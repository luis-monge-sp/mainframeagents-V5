/***                                                          ***/      00200000
/****************************************************************/      00210000
                                                                        00220000
IF ( DATATYPE(LINE.0,N) ) THEN ,                                        00230000
    "EXECIO" LINE.0 "DISKW " DDN " ( STEM LINE. FINIS "                 00240000
                                                                        00250000
EXIT CMD_RC                                                             00260000
