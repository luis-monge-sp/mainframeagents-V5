/****************************************************************/      00100000
                                                                        00110000
X = OUTTRAP("LINE.","*", CONCAT )                                       00120000
CALL CTSBKRUL FUNC ADMIN RESTYPE RESNAME                                00130000
X = OUTTRAP('OFF')                                                      00140000
CMD_RC = RESULT                                                         00150000
                                                                        00160000
/****************************************************************/      00170000
/***                                                          ***/      00180000
