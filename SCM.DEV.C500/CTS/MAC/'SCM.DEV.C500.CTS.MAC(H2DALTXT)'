         MACRO                                                          00010000
&NAME    H2DALTXT &VERB=,&PREFIX=,&FIELDS=(PTR,UNT),&MF=L,             +00020000
               &HELP=NO,                                               +00030000
               &BFALN=,&BFTEK=,&BLKLN=,&BLKSZ=,&BUFIN=,&BUFL=,&BUFMX=, +00040000
               &BUFNO=,&BUFOF=,&BUFOU=,&BUFRQ=,&BUFSZ=,                +00050000
               &CDISP=,&CLOSE=,&CNVRT=,&CODE=,&COPYS=,&CPRI=,&CURNT=,  +00060000
               &CYL=,                                                  +00070000
               &DEFER=,                                                +00080000
               &DCBDD=,&DCBDS=,&DDNAM=,&DEN=,&DIAGN=,&DIR=,&DSNAM=,    +00090000
               &DSORG=,&DSSEQ=,&DUMMY=,                                +00100000
               &EROPT=,&EXPDT=,&FCBAV=,&FCBIM=,&FRID=,&FUNC=,          +00110000
               &GNCP=,&INOUT=,&INTVL=,&IPLTX=,&KYLEN=,                 +00120000
               &LABEL=,&LIMCT=,&LRECL=,&MEMBR=,&MODE=,&MSVGP=,         +00130000
               &NCP=,&NDISP=,                                          +00140000
               &OPTCD=,&OUTLM=,&OVCLS=,&OVDSP=,&OVSHQ=,&OVSNH=,&OVSUS=,+00150000
