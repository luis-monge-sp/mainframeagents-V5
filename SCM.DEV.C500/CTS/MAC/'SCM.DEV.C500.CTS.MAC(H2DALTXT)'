         AIF   (&I EQ &H2DLINX).MFLLPTR                                 07260000
         DC    A(&H2DLPRF&H2DLUNT(&I))                                  07270000
         AGO   .OLPPTR                                                  07280000
.MFLLPTR ANOP                                LAST ENTRY MF=L            07290000
         DC    XL1'80' -  LAST ENTRY INDICATION                         07300000
         DC    AL3(&H2DLPRF&H2DLUNT(&I)) -  LAST ENTRY IN LIST          07310000
         AGO   .OLPPTR                                                  07320000
.MFEPTRE ANOP                                MF=E ENTRY IN LIST         07330000
         LA    0,&H2DLPRF&H2DLUNT(&I)                                   07340000
         ST    0,0(,1)                                                  07350000
         AIF   (&I EQ &H2DLINX).MFELPTR                                 07360000
         LA    1,4(,1)                                                  07370000
         AGO   .OLPPTR                                                  07380000
.MFELPTR ANOP                                LAST ENTRY MF=E            07390000
         OI    0(1),X'80' -  SET ON LAST ENTRY INDICATION               07400000
.OLPPTR  ANOP                                                           07410000
&I       SETA  &I+1                                                     07420000
         AIF   (&I LE &H2DLINX).LPPTR                                   07430000
         AGO   .TXTUNT                       AFTER TUPL HANDLING,       07440000
.*                                           ATTEMPT A TEXT UNIT HNDLNG 07450000
.*-------------                                                         07460000
.*       ERROR  HANDLING                                                07470000
.*-------------                                                         07480000
.ERMFINV MNOTE 8,'*** H2DALTXT *** MF=&MF IS ILLEGAL'                   07490000
         MEXIT                                                          07500000
.ERFLDIN MNOTE 8,'*** H2DALTXT *** FIELDS=&FIELDS IS ILLEGAL'           07510000
         MEXIT                                                          07520000
