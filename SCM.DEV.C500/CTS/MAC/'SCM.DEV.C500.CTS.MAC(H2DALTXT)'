               &PRIME=,&PRIVT=,&PRTSP=,&QNAME=,                        +00170000
               &RECFM=,&RELNO=,&REMOV=,&RETPD=,&RLSE=,&ROUND=,&RSRVF=, +00180000
               &RSRVS=,&RTATT=,&RTCDP=,&RTDDN=,&RTDSN=,&RTDUM=,&RTLIM=,+00190000
               &RTLST=,&RTMEM=,&RTNDP=,&RTORG=,&RTSTA=,&RTTYP=,&RTVOL=,+00200000
               &SECND=,&SFMNO=,&SHOLD=,&SOWA=,&SPFRM=,&SPGNM=,&SSREQ=, +00210000
               &STACK=,&STATS=,&SUSER=,&SYSOU=,                        +00220000
               &TCBAD=,&TERM=,&THRSH=,&TRK=,&TRTCH=,                   +00230000
               &UCS=,&UFOLD=,&UNALC=,&UNCNT=,&UNIT=,&UVRFY=,           +00240000
               &VLCNT=,&VLRDS=,&VLSEQ=,&VLSER=                          00250000
.*--------------------------------------------------------------------* 00260000
.*       S E T    S Y M B O L S    D E F I N I T I O N S              * 00270000
.*--------------------------------------------------------------------* 00280000
         GBLA  &H2DLINX                      INDEX IN UNTNAMES VECTOR   00290000
         GBLC  &H2DLUNT(100)                 UNTNAMES VECTOR            00300000
         GBLC  &H2DLPRF                      HOLDS PREFIX FOR IN-MACRO  00310000
         GBLB  &H2DLBMF                      MF=E                       00320000
         GBLB  &H2DLBPT                      FIELDS=PTR                 00330000
         GBLB  &H2DLBUN                      FIGLDS=UNT/KEY/LEN/PRM     00340000
         GBLB  &H2DLBKY                      FIELDS=KEY                 00350000
         GBLB  &H2DLB#                       FIELDS=UNT                 00360000
         GBLB  &H2DLBLN                      FIELDS=LEN                 00370000
         GBLB  &H2DLBPR                      FIELDS=LEN/PRM             00380000
         GBLB  &H2DALNL            RETURNS FROM H2DALNUL                00390000
         LCLA  &N,&I                                                    00400000
         LCLB  &BPT,&BUN,&BKY,&B#,&BLN,&BPR  INDICATIONS                00410000
.*-------------                                                         00420000
.*       INIT  GLOBAL SET SYMBOLS                                       00430000
.*-------------                                                         00440000
&H2DLINX SETA  0                                                        00450000
&H2DLPRF SETC  ''                                                       00460000
&H2DLBMF SETB  0                                                        00470000
.*--------------------------------------------------------------------* 00480000
.*       " H E L P "    D O C U M A N T A T I O N                     * 00490000
.*--------------------------------------------------------------------* 00500000
.*                                                                      00510000
         AIF   ('&HELP' EQ 'NO').OVHELP                                 00520000
*---------------------------------------------------------------------* 00530000
*        D Y N A M I C    A L L O C A T I O N    M A C R O            * 00540000
*        H 2 D A L T X T                                              * 00550000
*        THIS MACRO DEFINES/MODIFIES:                                 * 00560000
*        1. TEXT UNIT POINTERS LIST;                                  * 00570000
*        2. TEXT UNITS;                                               * 00580000
*        MACROS USED IN THIS MACRO DEFINITION:                        * 00590000
*        1. H2DALUNT - TO DEFINE/MODIFY A TEXT UNIT;                  * 00600000
*        2. H2DALSCN - TO TRANSLATE A MNEUMONIC INTO A BIT COMBINATION* 00610000
*                     (USED ONLY BY H2DALUNT)                         * 00620000
*        3. H2DALNUL - TO SCAN PARAMETER LIST FOR ALL NULL.             00630000
*        MACRO PARAMETERS ARE DEVIDED INTO TWO GROUPS:                * 00640000
*        1. CONTROL PARAMETERS, WHICH AFFECT  FLOW OF MACRO DEFINITION* 00650000
*        2. ALLOCATION PARAMETERS, WHICH DETERMINE WHICH TEXT UNITS   * 00660000
*           AND WHICH TEXT UNIT POINTERS WILL BE AFFECTED.            * 00670000
*        CONTROL PARAMETERS ARE:                                      * 00680000
*        &HELP=   - DETERMINES WHETHER THESE COMMENTS ARE TO BE PRINTD* 00690000
*                   NO  - COMMENTS ARE NOT PRINTED;                   * 00700000
*                   YES - COMMENTS ARE PRINTED TO HELP PROGRAMMER;    * 00710000
*                   DEFAULT IS &HELP=NO.                              * 00720000
*        &PREFIX= - SUPPLIES A PREFIX FOR SYMBOLIC NAMES (LABELS)     * 00730000
*                   AFFECTED BY H2DALTXT. MAY BE 1 TO 3 ALPHA-MERIC   * 00740000
*                   + NATIONALS, THE 1ST OF WHICH MAY NOT BE NUMERIC. * 00750000
*                   DEFAULT IS &PREFIX=''.                            * 00760000
*        &VERB=   - DETERMINES DYNAMIC ALLOCATION VERB. FOR EACH VERB * 00770000
*                   THERE IS A SET OF ALLOCATION PARAMETERS.          * 00780000
*                   FOLLWING ARE THE VARIOUS VERBS POSSIBLE:          * 00790000
*                   AL - DYNAMIC ALLOCATION BY DSNAME;                * 00800000
*                   UN - DYNAMIC UNALLOCATION;                        * 00810000
*                   CC - DYNAMIC CONCATENATION;                       * 00820000
*                   DC - DYNAMIC DECONCATENATION;                     * 00830000
*                   RI - REMOVE IN-USE ATTRIBUTE;                     * 00840000
*                   DN - DDNAME ALLOCATION;                           * 00850000
*                   IN - INFORMATION RETRIEVAL;                       * 00860000
*                   THIS PARAMETER IS OBLIGATORY. THERE IS NO DEFAULT-* 00870000
*                   OPTION FOR &VERB.                                 * 00880000
*        &FIELDS= - DETERMINES FIELDS AFFECTED BY THIS MACRO:         * 00890000
*                   PTR - TEXT UNIT POINTERS LIST IS AFFECTED;        * 00900000
*                   UNT - ALL FIELDS IN TEXT UNITS ARE AFFECTED;      * 00910000
*                   KEY - ONLY THE KEY FIELD IS AFFECTED;             * 00920000
*                   LEN - LEN & PARM FIELDS ARE AFFECTED;             * 00930000
*                   PRM - ONLY PARM FIELD IS AFFECTED;                * 00940000
*                   DEFAULT IS FIELDS=(PTR,UNT)                       * 00950000
*        &MF=     - MACRO FORM.                                       * 00960000
*                   L - THE STANDARD MACRO FORM, TO DEFINE TABLES     * 00970000
*                   E - EXECUTE FORM, TO CREATE OR MODIFY TABLES      * 00980000
*                   DEFAULT IS MF=L.                                  * 00990000
*        ALLOCATION PARAMETERS ARE OF THE FORMAT                      * 01000000
*        <KEYWORD>=<VALUE>                                            * 01010000
*        <KEYWORD> STANDS FOR AN ALLOCATION PARAMETER NAME.           * 01020000
*        <VALUE>   STANDS FOR PARAMETER'S VALUE.                      * 01030000
*        CODING RULES FOR <VALUE>:                                    * 01040000
*                   *    TEXT UNIT WITH A NULL PARM FIELD;            * 01050000
*                  (*,N) "N" NULL PARM FIELDS;                        * 01060000
*                   "    TEXT UNIT WITH NULL KEY & PARM FIELDS;       * 01070000
*                  (",N) A NULL KEY FIELD AND "N" NULL PARM FIELDS;   * 01080000
*                   YES/Y FOR TEXT UNIT WITH NO LEN/PARM FIELD;       * 01090000
*                   CHARACTER-STRING FOR CHARACTERIAL TEXT UNIT;      * 01100000
*                   NUMBER FOR NUMERAL TEXT UNIT;                     * 01110000
*                   MNEUMONIC (SHR,KEEP,FB) FOR BIT COMBINATION;      * 01120000
*        NOTE: FOR TEXT UNIT OF INFORMATION RETIEVAL (RTVOL,RTDDN),   * 01130000
*              CODE *.                                                * 01140000
*              FOR MORE THAN ONE PARAMETER, CODE ALL VALUES WITHIN    * 01150000
*              PARENTHESIS, SEPERATED BY COMMAS                       * 01160000
*---------------------------------------------------------------------* 01170000
.OVHELP  ANOP                                                           01180000
.*--------------------------------------------------------------------- 01190000
.*       A N A L Y Z E    P A R A M E T E R S                         * 01200000
.*--------------------------------------------------------------------- 01210000
.*                                                                      01220000
.*-------------                                                         01230000
.*       NAME  FIELD                                                    01240000
.*-------------                                                         01250000
         AIF   ('&NAME' EQ '').ONAMFLD                                  01260000
         CNOP  0,4                                                      01270000
&NAME    EQU   *                                                        01280000
.ONAMFLD ANOP                                                           01290000
.*-------------                                                         01300000
.*       PREFIX  PARAMETER                                              01310000
.*-------------                                                         01320000
&H2DLPRF SETC  '&PREFIX'                     SAVE FOR INSIDE MACRO USE  01330000
.*                                                                      01340000
.******  MF  PARAMETER                                                  01350000
.*                                                                      01360000
         AIF   ('&MF' EQ 'L').OANLMF         H2DLBMF=0 FOR MF=L         01370000
         AIF   ('&MF' NE 'E').ERMFINV        ERROR-MF IS INVALID        01380000
&H2DLBMF SETB  1                             H2DLBMF=1 FOR MF=E         01390000
.OANLMF  ANOP                                                           01400000
.*-------------                                                         01410000
.*       FIELDS  PARAMETER                                              01420000
.*-------------                                                         01430000
         AIF   ('&FIELDS' EQ '').ERFLDIN     ERROR- FIELDS NULLIFIED    01440000
&N       SETA  N'&FIELDS                                                01450000
&I       SETA  0                                                        01460000
.FLDLOOP ANOP                                LOOP ON FIELDS SUBPARM'S   01470000
&I       SETA  &I+1                                                     01480000
         AIF   (&I GT &N).FLDCHK                                        01490000
         AIF   ('&FIELDS(&I)' EQ 'PTR').FLDPTR                          01500000
         AIF   ('&FIELDS(&I)' EQ 'UNT').FLDUNT                          01510000
         AIF   ('&FIELDS(&I)' EQ 'KEY').FLDKEY                          01520000
         AIF   ('&FIELDS(&I)' EQ 'LEN').FLDLEN                          01530000
         AIF   ('&FIELDS(&I)' EQ 'PRM').FLDPRM                          01540000
         AGO   .ERFLDIN                      ERROR- FIELDS IS INVALID   01550000
.FLDPTR  ANOP                                FIELDS=PTR                 01560000
&BPT     SETB  1                                                        01570000
         AGO   .FLDLOOP                                                 01580000
.FLDUNT  ANOP                                FIELDS=UNT                 01590000
&BUN     SETB  1                                                        01600000
&BKY     SETB  1                                                        01610000
&B#      SETB  1                                                        01620000
&BLN     SETB  1                                                        01630000
&BPR     SETB  1                                                        01640000
         AGO   .FLDLOOP                                                 01650000
.FLDKEY  ANOP                                FIELDS=KEY                 01660000
         AIF   (NOT &H2DLBMF).ERFLDMF        ERROR- FIELDS ILLEGAL MF=L 01670000
&BUN     SETB  1                                                        01680000
&BKY     SETB  1                                                        01690000
         AGO   .FLDLOOP                                                 01700000
.FLDLEN  ANOP                                FIELDS=LEN                 01710000
         AIF   (NOT &H2DLBMF).ERFLDMF        ERROR- FIELDS ILLEGAL MF=L 01720000
&BUN     SETB  1                                                        01730000
&BLN     SETB  1                                                        01740000
&BPR     SETB  1                                                        01750000
         AGO   .FLDLOOP                                                 01760000
.FLDPRM  ANOP                                FIELDS=PRM                 01770000
         AIF   (NOT &H2DLBMF).ERFLDMF        ERROR- FIELDS ILLEGAL MF=L 01780000
&BUN     SETB  1                                                        01790000
&BPR     SETB  1                                                        01800000
         AGO   .FLDLOOP                                                 01810000
.FLDCHK  ANOP                                                           01820000
.*-------------                                                         01830000
.*       THE FOLLOWING CODE FNABLES MACRO FIRST TO GENERATE TEXT-UNIT-  01840000
.*       POINTERS-LIST AND THEN THE REQUESTED TEXT UNITS.               01850000
.*       CONSEQENCIALY, MF=E IS ALSO AFFECTED, SO LIST IS HANDLED       01860000
.*       PRIMERLY.                                                      01870000
.*-------------                                                         01880000
&H2DLBUN SETB  0                                                        01890000
&H2DLBKY SETB  0                                                        01900000
&H2DLB#  SETB  0                                                        01910000
&H2DLBLN SETB  0                                                        01920000
&H2DLBPR SETB  0                                                        01930000
&H2DLBPT SETB  (&BPT)                                                   01940000
         AIF   (&H2DLBPT).OANLFLD            GO TO HANDLE TUPL.         01950000
.TXTUNT  ANOP                                COME BACK HERE FOR TEXTUNT 01960000
&H2DLBPT SETB  0                                                        01970000
&H2DLBUN SETB  (&BUN)                                                   01980000
&H2DLBKY SETB  (&BKY)                                                   01990000
&H2DLB#  SETB  (&B#)                                                    02000000
&H2DLBLN SETB  (&BLN)                                                   02010000
&H2DLBPR SETB  (&BPR)                                                   02020000
         AIF   (NOT &H2DLBUN).MEND                                      02030000
.OANLFLD ANOP                                END OF FIELDS  ANALYZING   02040000
.*-------------                                                         02050000
.*       DIVERT  ACCORDING  TO  VERB                                    02060000
.*-------------                                                         02070000
         AIF   ('&VERB' EQ 'AL').VERBAL                                 02080000
         AIF   ('&VERB' EQ 'UN').VERBUN                                 02090000
         AIF   ('&VERB' EQ 'CC').VERBCC                                 02100000
         AIF   ('&VERB' EQ 'DC').VERBDC                                 02110000
         AIF   ('&VERB' EQ 'RI').VERBRI                                 02120000
         AIF   ('&VERB' EQ 'DN').VERBDN                                 02130000
         AIF   ('&VERB' EQ 'IN').VERBIN                                 02140000
         AGO   .ERVRBIN                      ERROR- VERB IS INVALID     02150000
.*--------------------------------------------------------------------- 02160000
